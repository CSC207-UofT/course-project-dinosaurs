<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controllers.MainGUIController"
            prefHeight="151.0" prefWidth="400.0">

        <Label alignment="CENTER" layoutX="32.0" layoutY="36.0" text="Save all current Checklists?">
            <font>
                <Font name="Calibri" size="18.0" />
            </font>
        </Label>
        <HBox alignment="CENTER" layoutX="100.0" layoutY="64.0" prefHeight="75.0" prefWidth="200.0" spacing="20.0">

                <Button mnemonicParsing="false" onAction="#saveChecklistButton" text="Yes, Save All" />
                <Button mnemonicParsing="false" onAction="#popUpCancelButton" text="No, Go Back" />

        </HBox>


</AnchorPane>
