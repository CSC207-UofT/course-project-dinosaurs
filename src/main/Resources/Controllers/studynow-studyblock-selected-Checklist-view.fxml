<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="454.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.studyNowSBChecklistController">
    <Label layoutX="249.0" layoutY="14.0" prefHeight="65.0" prefWidth="100.0" text="Checklist Options:" />
    <VBox layoutX="57.0" layoutY="358.0" prefHeight="96.0" prefWidth="382.0" spacing="10.0">
        <HBox prefHeight="25.0" prefWidth="200.0" spacing="10.0" />
        <HBox prefHeight="25.0" prefWidth="200.0" spacing="10.0">
            <Button mnemonicParsing="false" onAction="#saveSBfromCheckList" prefHeight="27.0" prefWidth="52.0" text="Save" />
            <HBox prefHeight="25.0" prefWidth="200.0" spacing="10.0">
                <Button mnemonicParsing="false" onAction="#changeSceneToBlockManagerButton" text="Cancel"/>
            </HBox>
        </HBox>
    </VBox>
    <Label layoutX="57.0" layoutY="318.0" text="Length (in minutes):" />
    <TextField fx:id="studyBlockLengthTextField" layoutX="220.0" layoutY="313.0" prefHeight="27.0" prefWidth="161.0" promptText="int greater than 0" />
    <Label layoutX="57.0" layoutY="281.0" text="Enter Name of Study Block: " />
    <TextField fx:id="SBfromCheckListNameTextField" layoutX="220.0" layoutY="276.0" promptText="name" />
   <ListView fx:id="listView" layoutX="158.0" layoutY="79.0" prefHeight="178.0" prefWidth="283.0" />
</AnchorPane>
